alias tmux="TERM=xterm-256color tmux"

alias rabbit='sudo rabbitmqctl'

alias tnd='tmuxp load -2 $HOME/.tmuxp/dev.yaml'
alias tad='tmux -2 a -t dev'
alias tkd='tmux kill-session -t dev'

alias tns='tmuxp load -2 $HOME/.tmuxp/scratchpad.yaml'
alias tas='tmux -2 a -t scratchpad'
alias tks='tmux kill-session -t scratchpad'

alias tnp='tmuxp load -2 $HOME/.tmuxp/prod.yaml'
alias tap='tmux -2 a -t prod'
alias tkp='tmux kill-session -t prod'

alias tnpr='tmuxp load -2 $HOME/.tmuxp/prod_release.yaml'
alias tapr='tmux -2 a -t prod-release'
alias tkpr='tmux kill-session -t prod-release'

alias tls='tmux ls'

alias clear_pyc='find . -name "*.pyc" -exec rm -rf {} \;'

alias gf='git fetch'
alias gbvv='git branch -vv'
alias gstl='git stash list'
alias gcp='git cherry-pick'
alias gcpa='git cherry-pick --abort'
alias gcpc='git cherry-pick --continue'
alias gbg='gb | grep --color=auto'
alias gbrg='gbr | grep --color=auto'

alias lsg='ls | grep --color=auto --exclude-dir={.bzr,.cvs,.git,.hg,.svn}'
alias psg='ps aux | grep --color=auto'

alias ff='find . -name '

# WebInterpret aliases wi-webstores
alias run_store_manager='webstores-admin --conf=components/store_manager/settings/defaults.py run_store_manager'
alias run_store_connector_frontend='webstores-admin --conf=components/store_connector/settings/multiple_test_platforms.py run_store_connector_frontend'
alias run_frontend_magento_test='webstores-admin --conf=settings/test_store.py run_frontend --locale=de --tenant="magento-test"'
alias run_workers='webstores-admin --conf=settings/test_store.py run_workers'
alias run_fetching='webstores-admin --conf=settings/test_store.py run_fetching'

